// cSpell Settings
{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en,en-GB",
    "enableGlobDot": true,
    "useGitignore": true,
    "dictionaries": [
        "poe-dict",
        "pob-dict",
        "ignore-dict",
        "!aws",
        "!cryptocurrencies",
        "!ada"
    ],
    "dictionaryDefinitions": [
        {
            "name": "poe-dict",
            "path": "./poe-dict.txt",
            "description": "Path of Exile dictionary",
            "addWords": true
        },
        {
            "name": "pob-dict",
            "path": "./pob-dict.txt",
            "description": "Path Of Building Community dictionary",
            "addWords": true
        },
        {
            "name": "ignore-dict",
            "path": "./ignore-dict.txt",
            "description": "Path Of Building Community ignore dictionary",
            "addWords": true
        }
    ],
    "ignoreRegExpList": [
        "/\\^x[\\d\\w]{6}/",
        "/manifeste\\?/"
    ],
    "globRoot": "${cwd}",
    "ignorePaths": [
        "*.dll",
        "*.exe",
        "*.png",
        "*.tga",
        "*.zip.part*",
        "*.zip",
        ".git",
        ".gitattributes",
        "cspell.json",
        "./runtime/*",
        "./src/Builds/*",
        "./src/Settings.xml", // User file
        /* ************************ Auto-generated files from here down ************************ */
        "./src/Data/Bases/*",
        "./src/Data/ClusterJewels.lua",
        "./src/Data/Costs.lua",
        "./src/Data/EnchantmentBelt.lua",
        "./src/Data/EnchantmentBody.lua",
        "./src/Data/EnchantmentBoots.lua",
        "./src/Data/EnchantmentFlask.lua",
        "./src/Data/EnchantmentGloves.lua",
        "./src/Data/EnchantmentHelmet.lua",
        "./src/Data/EnchantmentWeapon.lua",
        "./src/Data/Essence.lua",
        "./src/Data/Gems.lua",
        "./src/Data/LegionPassives.lua",
        "./src/Data/Minions.lua",
        "./src/Data/Misc.lua",
        "./src/Data/ModCache.lua",
        "./src/Data/ModFlask.lua",
        "./src/Data/ModItem.lua",
        "./src/Data/ModJewel.lua",
        "./src/Data/ModJewelAbyss.lua",
        "./src/Data/ModJewelCluster.lua",
        "./src/Data/ModMaster.lua",
        "./src/Data/ModVeiled.lua",
        "./src/Data/Pantheons.lua",
        "./src/Data/Skills/*.lua",
        "./src/Data/Spectres.lua",
        "./src/Data/StatDescriptions/*",
        "./src/Data/TimelessJewelData/*",
        "./src/Data/Uniques/Special/WatchersEye.lua",
        "./src/Export/Minions/*",
        "./src/Export/spec.lua",
        "./src/Export/Tree/*",
        "./src/TreeData/*"
    ],
    "flagWords": [
        "and and",
        "Casted",
        "is is",
        "it it",
        "the the",
        "youd"
    ],
    "enableFiletypes": [
        "ignore",
        "properties",
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "html",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "markdown",
        "php",
        "plaintext",
        "python",
        "text",
        "typescript",
        "typescriptreact",
        "vue",
        "yaml",
        "yml",
        "lua",
        "xml"
    ]
}
